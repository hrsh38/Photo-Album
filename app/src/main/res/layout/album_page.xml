<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button14"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/right"
        android:layout_alignBottom="@+id/right"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/imageView"
        android:layout_x="1dp"
        android:layout_y="131dp"
        android:onClick="goLeft"
        android:text="L" />

    <Button
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_x="4dp"
        android:layout_y="463dp"
        android:onClick="Cancels"
        android:text="Back" />

    <Button
        android:id="@+id/remove"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button7"
        android:layout_alignBottom="@+id/button7"
        android:layout_centerHorizontal="true"
        android:layout_weight="1"
        android:layout_x="145dp"
        android:layout_y="391dp"
        android:onClick="delete"
        android:text="Remove" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="59dp"
        android:layout_marginStart="22dp"
        android:layout_toEndOf="@+id/button8"
        android:layout_weight="1"
        android:layout_x="263dp"
        android:layout_y="393dp"
        android:onClick="display"
        android:text="Display" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="271dp"
        android:layout_height="228dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:layout_x="48dp"
        android:layout_y="43dp"
        android:cropToPadding="false"
        app:srcCompat="@android:drawable/bottom_bar" />

    <TextView
        android:id="@+id/tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignStart="@+id/button8"
        android:layout_x="137dp"
        android:layout_y="285dp"
        android:text="Person, Location" />

    <Button
        android:id="@+id/left"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/right"
        android:layout_alignBottom="@+id/right"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/imageView"
        android:layout_x="317dp"
        android:layout_y="133dp"
        android:onClick="goRight"
        android:text="R" />

    <Button
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignStart="@+id/button8"
        android:layout_x="22dp"
        android:layout_y="391dp"
        android:text="Add Photo" />

    <Button
        android:id="@+id/button13"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:layout_x="49dp"
        android:layout_y="323dp"
        android:elevation="24dp"
        android:onClick="addTag"
        android:text="Add Tag" />

    <Button
        android:id="@+id/button8"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:layout_x="225dp"
        android:layout_y="324dp"
        android:elevation="24dp"
        android:onClick="deleteTag"
        android:text="Delete Tag" />

    android:layout_alignBaseline="@+id/remove"
        android:layout_alignBottom="@+id/remove"
        android:layout_marginEnd="23dp"
        android:layout_toStartOf="@+id/button8"
        android:layout_weight="1"
        android:layout_x="36dp"
        android:layout_y="389dp"
        android:elevation="0dp"
        android:text="Add" />


</AbsoluteLayout>

