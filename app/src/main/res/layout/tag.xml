<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:id="@+id/button"
        android:layout_width="104dp"
        android:layout_height="64dp"
        android:layout_x="38dp"
        android:layout_y="421dp"
        android:onClick="saveTag"
        android:text="Save" />

    <Button
        android:id="@+id/button3"
        android:layout_width="107dp"
        android:layout_height="62dp"
        android:layout_x="234dp"
        android:layout_y="423dp"
        android:onClick="Cancel"
        android:text="Cancel" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="49dp"
        android:layout_y="152dp"
        android:text="Person" />

    <EditText
        android:id="@+id/person"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_x="41dp"
        android:layout_y="189dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="46dp"
        android:layout_y="260dp"
        android:text="Location" />

    <EditText
        android:id="@+id/location"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_x="43dp"
        android:layout_y="289dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="263dp"
        android:layout_height="76dp"
        android:layout_x="67dp"
        android:layout_y="41dp"
        android:text="Tags Page"
        android:textColor="#a82121"
        android:textSize="40dp" />
</AbsoluteLayout>